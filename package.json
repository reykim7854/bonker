{
  "name": "bonker",
  "private": true,
  "version": "1.0.0",
  "description": "Bonker",
  "repository": "",
  "license": "UNLICENSED",
  "scripts": {
    "start": "pnpm run dev",
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production vite build && npx workbox generateSW workbox-config.js",
    "build-capacitor-ios": "cross-env NODE_ENV=production vite build && npx cap copy ios",
    "build-capacitor-android": "cross-env NODE_ENV=production vite build && npx cap copy android",
    "postinstall": "cpy --flat ./node_modules/framework7-icons/fonts/*.* ./src/fonts/ && cpy --flat ./node_modules/material-icons/iconfont/*.* ./src/fonts/",
    "lint": "pnpm exec eslint --fix . --ext .js,.svelte",
    "test": "pnpm exec vitest",
    "test:ui": "pnpm exec vitest --ui --coverage.enabled=true",
    "coverage": "pnpm exec vitest run --coverage",
    "prepare": "pnpm exec husky install",
    "test-ct": "pnpm exec playwright test -c playwright-ct.config.js",
    "test-ct:ui": "pnpm exec playwright test -c playwright-ct.config.js --ui",
    "test-e2e": "pnpm exec playwright test -c playwright.config.js",
    "test-e2e:ui": "pnpm exec playwright test -c playwright.config.js --ui"
  },
  "lint-staged": {
    "*.{js,svelte}": [
      "eslint --quiet --fix"
    ]
  },
  "browserslist": [
    "IOS >= 15",
    "Safari >= 15",
    "last 5 Chrome versions",
    "last 5 Firefox versions"
  ],
  "dependencies": {
    "@capacitor/android": "^5.4.2",
    "@capacitor/app": "^5.0.6",
    "@capacitor/browser": "^5.1.0",
    "@capacitor/core": "^5.4.2",
    "@capacitor/ios": "^5.4.2",
    "@capacitor/keyboard": "^5.0.6",
    "@capacitor/splash-screen": "^5.0.6",
    "@capacitor/status-bar": "^5.0.6",
    "dexie": "^3.2.4",
    "dom7": "^4.0.6",
    "framework7": "^8.3.0",
    "framework7-icons": "^5.0.5",
    "framework7-svelte": "^8.3.0",
    "material-icons": "^1.13.12",
    "skeleton-elements": "^4.0.1",
    "svelte": "^4.2.1",
    "svelte-i18n": "^3.7.4",
    "swiper": "^10.3.1"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.4.2",
    "@eslint/create-config": "^0.4.6",
    "@playwright/experimental-ct-svelte": "^1.39.0",
    "@playwright/test": "^1.39.0",
    "@sveltejs/vite-plugin-svelte": "^2.4.6",
    "@types/node": "^20.8.6",
    "@vitest/coverage-istanbul": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "cordova-res": "^0.15.4",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-svelte": "^2.34.0",
    "happy-dom": "^12.9.1",
    "husky": "^8.0.0",
    "husky-init": "^8.0.0",
    "lint-staged": "^14.0.1",
    "postcss-preset-env": "^9.1.4",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "sass": "^1.69.0",
    "vite": "^4.4.11",
    "vitest": "^0.34.6",
    "workbox-cli": "^7.0.0"
  }
}
